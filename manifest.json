{
    "name": "personalized-alaram",
    "version": "0.0.1",
    "description": "create personalized alaram",
    "manifest_version": 2,
    "icons":{
        "16": "assets/images/icon16.png",
        "32": "assets/images/icon32.png",
        "48": "assets/images/icon48.png",
        "128": "assets/images/icon128.png"
    },
    "browser_action": {
        "default_popup": "popup.html",
        "browser_style": true
      },
      "permissions": [
        "storage",
        "alarms",
        "notifications"
      ],
      "browser_specific_settings": {
        "gecko": {
          "id": "manojramoorthy@gmail.com",
          "strict_min_version": "42.0"
        }
      },
      "options_ui": {
        "page": "options.html",
        "browser_style": false
      },
      "background": {
        "scripts": ["assets/js/background.js"]
      }
}